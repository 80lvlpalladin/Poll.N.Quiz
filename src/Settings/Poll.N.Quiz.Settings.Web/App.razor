@using Poll.N.Quiz.Settings.Web.Pages

<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(SettingsViewer)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(SettingsViewer)">
            <p>Page not found</p>
        </LayoutView>
    </NotFound>
</Router>
